<Demos:UDPDemo
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:Demos="clr-namespace:RTSScreenWPF"
	mc:Ignorable="d"
	x:Class="RTSScreenWPF.StreamConsoleDemo"
	d:DesignWidth="640" d:DesignHeight="480">

	<Grid x:Name="LayoutRoot">
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="0.5*"/>
			<ColumnDefinition Width="0.5*"/>
		</Grid.ColumnDefinitions>
		<Rectangle Fill="Black" Stroke="Black"/>
		<TextBlock x:Name="console" TextWrapping="Wrap" FontFamily="Consolas" FontSize="26.667" Foreground="White" Margin="8"/>
		<Image x:Name="graphics" Grid.Column="1" Margin="8"/>
	</Grid>
</Demos:UDPDemo>